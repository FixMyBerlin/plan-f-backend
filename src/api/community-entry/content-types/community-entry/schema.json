      "target": "api::measure.measure",
      "inversedBy": "communityEntries"
    },
    "subcategory": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "contact": {
      "type": "string",
      "required": false
    },
    "countryState": {
      "type": "string",
      "required": true
    },
    "commune": {
      "type": "string",
      "required": true
    }
